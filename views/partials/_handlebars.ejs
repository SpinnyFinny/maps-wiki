<!-- Handlebars Template -->
<script id="infowindow-template" type="text/x-handlebars-template">
  {{#each this}}
  "<ul>
      <li>latitude: {{data.latitude}}</li>
      <li>longitude: {{data.longitude}}</li>
      <li>title: {{data.title}}</li>
      <li>description: {{data.description}}</li>
      <li>image: <img src="{{data.image}}" /></li>
      <li>url: <a href="{{data.url}}">{{data.url}}</a></li>
      <li>user_id: {{data.user_id}}</li>
      <li>type_id: {{data.type_id}}</li>
      <li>map_id: {{data.map_id}}</li>
      <li>created_at: {{data.created_at}}</li>
      <li>version: {{data.version}}</li>
    </ul>"
  {{/each}}
</script>